{
  "name": "alibaba-project-ssr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npx json-server db.json\"",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "build": "vite build",
    "server": "node --loader ts-node/esm ./server/index.ts",
    "server:dev": "npm run server",
    "server:prod": "cross-env NODE_ENV=production npm run server",
    "server:prod:docker": "concurrently \"yarn run server:prod\" \"npx json-server db.json\"",
    "lint": "eslint . --max-warnings 0",
    "dev:docker": "docker compose -f ./docker/docker-compose.yaml up -d",
    "preview": "vite preview --port 3001",
    "type-check": "tsc --skipLibCheck --noEmit",
    "test": "vitest",
    "lint-check": "eslint ."
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.18.2",
    "json-server": "^1.0.0-beta.3",
    "leaflet": "^1.9.4",
    "postcss": "^8.5.3",
    "react": "^19.0.0-rc.1",
    "react-dom": "^19.0.0-rc.1",
    "react-leaflet": "^5.0.0-rc.2",
    "sirv": "^2.0.3",
    "tailwindcss": "^4.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^9.1.2",
    "vite-plugin-inject-preload": "^1.3.3",
    "vitest": "^3.0.6"
  }
}
